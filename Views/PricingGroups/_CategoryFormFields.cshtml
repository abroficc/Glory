    <select class="v-select form-control selectpicker" id="@(ViewData["Prefix"])-group-type" required>
      <option value="">اختر نوع المجموعة</option>
      <option value="شمال">شمال</option>
      <option value="جنوب">جنوب</option>
    </select>
    <div class="invalid-feedback">الرجاء اختيار نوع المجموعة</div>
  </div>
  
