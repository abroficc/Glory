@model IEnumerable<Glory77.Models.SystemConnection>

@{
    Layout = "_VerticalLayout";
    ViewData["Title"] = "ربطيات الأنظمة";
    ViewData["Direction"] = "rtl";
}

@section styles {
    <!-- date range picker css -->
    <link href="~/plugins/daterangepicker/daterangepicker.css" rel="stylesheet" type="text/css">
    <!-- flatpickr css -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/flatpickr/dist/flatpickr.min.css">
    <link rel="stylesheet" type="text/css" href="https://npmcdn.com/flatpickr/dist/themes/material_blue.css">
    
    <link href="https://fonts.googleapis.com/css2?family=Cairo:wght@400;600&display=swap" rel="stylesheet">

    <!-- System Connections Final Unified CSS -->
    <link href="~/css/system-connections-final.css?v=@DateTime.Now.Ticks" rel="stylesheet" />

    <!-- Bootstrap Select CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-select@1.14.0-beta3/dist/css/bootstrap-select.min.css" />

    <!-- DataTables CSS -->
    <link href="https://cdn.datatables.net/1.13.6/css/dataTables.bootstrap5.min.css" rel="stylesheet">
    <link href="https://cdn.datatables.net/responsive/2.5.0/css/responsive.bootstrap5.min.css" rel="stylesheet">

    <!-- Font Awesome -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" rel="stylesheet">
}

<!-- Filter toggle button -->
<div class="mb-2">
    <button id="toggleFilters" type="button" class="btn btn-primary">
        <i class="ti ti-filter-off me-1"></i><span id="toggleFiltersText">إظهار الفلترة</span>
    </button>
</div>

<!-- Filter section -->
<div class="card mb-3" id="filterSection" style="display: none;">
    <div class="card-body p-2">
        <!-- Filter row -->
        <div class="row g-2 mb-2">
            <div class="col-md-2">
                <div class="searchable-field">
                    <input type="text" class="form-control form-control-sm" placeholder="اسم المزود" id="filter-provider-name">
                </div>
            </div>
            <div class="col-md-2">
                <div class="searchable-field">
                    <input type="text" class="form-control form-control-sm" placeholder="الشبكة" id="filter-network">
                </div>
            </div>
            <div class="col-md-2">
                <div class="searchable-field">
                    <input type="text" class="form-control form-control-sm" placeholder="الموظفين" id="filter-employees">
                </div>
            </div>
            <div class="col-md-2">
                <div class="searchable-field">
                    <input type="text" class="form-control form-control-sm" placeholder="المصادر الحسابية" id="filter-account-sources">
                </div>
            </div>
            <div class="col-md-2">
                <div class="searchable-field">
                    <select class="form-control form-control-sm" id="filter-direction">
                        <option value="">الجهة</option>
                        <option value="شمال">شمال</option>
                        <option value="جنوب">جنوب</option>
                    </select>
                </div>
            </div>
            <div class="col-md-2">
                <div class="searchable-field">
                    <input type="text" class="form-control form-control-sm" placeholder="الحالة" id="filter-status">
                </div>
            </div>
        </div>
        <div class="row g-2 mb-2">
            <div class="col-md-2">
                <div class="searchable-field">
                    <input type="text" class="form-control form-control-sm" placeholder="النوع" id="filter-type">
                </div>
            </div>
            <div class="col-md-2">
                <div class="searchable-field">
                    <input type="text" class="form-control form-control-sm" placeholder="رقم الحساب" id="filter-account-number">
                </div>
            </div>
            <div class="col-md-2">
                <div class="searchable-field">
                    <input type="text" class="form-control form-control-sm" placeholder="رقم الإيبي" id="filter-ip-number">
                </div>
            </div>
            <div class="col-md-3">
                <div class="searchable-field">
                    <input type="text" class="form-control form-control-sm" placeholder="تاريخ الإنشاء" id="filter-creation-date">
                </div>
            </div>
            <div class="col-md-3">
                <div class="searchable-field">
                    <input type="text" class="form-control form-control-sm" placeholder="تاريخ التحديث" id="filter-update-date">
                </div>
            </div>
        </div>
        <div class="row g-2">
            <div class="col">
                <div class="filter-buttons">
                    <button type="button" class="btn btn-secondary btn-sm" id="resetFilters">إعادة تعيين</button>
                    <button type="button" class="btn btn-primary btn-sm" id="searchButton">بحث</button>
                    <button type="button" class="btn btn-success btn-sm" id="exportExcelButton">تصدير اكسل</button>
                    <button type="button" class="btn btn-info btn-sm" id="printButton">طباعة</button>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Action buttons -->
<div class="mb-3">
    <div class="d-flex gap-2 flex-wrap">
        <a href="#" class="btn btn-sm btn-outline-danger" title="حذف">
            <i class="ti ti-trash"></i>
        </a>
        <a href="#" class="btn btn-sm btn-outline-success" title="إضافة" data-bs-toggle="modal" data-bs-target="#unifiedModal">
            <i class="ti ti-plus"></i>
        </a>
        <a href="#" class="btn btn-sm btn-outline-primary" title="تعديل" id="topEditButton">
            <i class="ti ti-pencil"></i>
        </a>
        <a href="#" class="btn btn-sm btn-outline-info" title="مشاهدة" id="topViewButton">
            <i class="ti ti-eye"></i>
        </a>
        <a href="#" class="btn btn-sm btn-outline-secondary" title="إعادة ترتيب المزودين" id="reorderProvidersButton">
            <i class="ti ti-arrows-sort"></i>
        </a>
