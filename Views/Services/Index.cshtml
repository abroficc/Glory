    <script>
        let addServiceEditor, editServiceEditor;
        
        // Initialize CKEditor for Add Service modal when it's shown
        document.getElementById('addServiceModal').addEventListener('shown.bs.modal', function () {
            if (!addServiceEditor) {
                ClassicEditor
                    .create(document.querySelector('#add-service-details'), {
                        language: 'ar',
                        toolbar: [
                            'heading', '|', 'bold', 'italic', 'underline', 'strikethrough',
                            '|', 'bulletedList', 'numberedList', 'blockQuote',
                            '|', 'link', 'insertTable', 'undo', 'redo'
                        ]
                    })
                    .then(editor => {
                        addServiceEditor = editor;
                        console.log('Add Service Editor initialized', editor);
                        
                        // Add blur event to validate on editor blur
                        editor.model.document.on('change:data', () => {
                            // Remove invalid state when user starts typing
                            const editorElement = document.querySelector('#add-service-details').closest('.ck-editor');
                            if (editorElement) {
                                editorElement.classList.remove('is-invalid');
                                const feedback = editorElement.nextElementSibling;
                                if (feedback && feedback.classList.contains('invalid-feedback')) {
                                    feedback.remove();
                                }
                            }
                        });
                    })
                    .catch(error => {
                        console.error('Add Service Editor error', error);
                    });
            }
        });

        // Initialize CKEditor for Edit Service modal when it's shown
        document.getElementById('editServiceModal').addEventListener('shown.bs.modal', function () {
            if (!editServiceEditor) {
                ClassicEditor
                    .create(document.querySelector('#edit-service-details'), {
                        language: 'ar',
                        toolbar: [
                            'heading', '|', 'bold', 'italic', 'underline', 'strikethrough',
                            '|', 'bulletedList', 'numberedList', 'blockQuote',
                            '|', 'link', 'insertTable', 'undo', 'redo'
                        ]
                    })
                    .then(editor => {
                        editServiceEditor = editor;
                        console.log('Edit Service Editor initialized', editor);
                        
                        // Add blur event to validate on editor blur
                        editor.model.document.on('change:data', () => {
                            // Remove invalid state when user starts typing
                            const editorElement = document.querySelector('#edit-service-details').closest('.ck-editor');
                            if (editorElement) {
                                editorElement.classList.remove('is-invalid');
                                const feedback = editorElement.nextElementSibling;
                                if (feedback && feedback.classList.contains('invalid-feedback')) {
                                    feedback.remove();
                                }
                            }
                        });
                    })
                    .catch(error => {
                        console.error('Edit Service Editor error', error);
                    });
            }
        });
    </script>

    <script src="~/js/services.js"></script>
}